<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox fx:id="Vboxtest" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <HBox>
         <children>
            <ImageView fx:id="DashBoardImage" fitHeight="43.0" fitWidth="264.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../../../Images/LibrarySystem.jpg" />
               </image>
            </ImageView>
            <Button mnemonicParsing="false" onAction="#lendingReceivingOnAction" text="Lending/Receiving" />
            <Button mnemonicParsing="false" text="Button" />
            <Button mnemonicParsing="false" text="Button" />
         </children>
      </HBox>
      <AnchorPane fx:id="dashBoardAnchorPane" prefHeight="200.0" prefWidth="200.0" />
   </children>
</VBox>
